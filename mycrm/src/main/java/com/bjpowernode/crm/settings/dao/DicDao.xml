<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bjpowernode.crm.settings.dao.DicDao">

    <select id="getAllDictype" resultType="com.bjpowernode.crm.settings.domain.DicType">
        select * from tbl_dic_type
    </select>

    <select id="getAllByDicType" resultType="com.bjpowernode.crm.settings.domain.DicValue">
        select * from tbl_dic_value where typeCode=#{code} order by orderNo
    </select>
</mapper>
























